{
  "name": "@murkl/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Murkl STARK verifier on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "stark",
    "zk",
    "zero-knowledge",
    "privacy",
    "murkl"
  ],
  "author": "murkl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exidz/murkl"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.98.4",
    "@solana/spl-token": "^0.4.14",
    "@noble/hashes": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": ">=0.29.0",
    "@solana/web3.js": ">=1.90.0"
  }
}
